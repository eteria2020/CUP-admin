        {
            "url": "ssh://git@git.omniaevo.it/sharengo-cartasi",
            "type": "git"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "~2.4",
        "bjyoungblood/bjy-authorize": "1.4.0",
        "zf-commons/zfc-user-doctrine-orm": "^1.0",
        "creof/doctrine2-spatial": "^0.0.1",
        "rwoverdijk/assetmanager": "1.4.*",
        "mvlabs/sharengo-coremodule": "dev-master",
        "leafo/lessphp": "0.5.*",
        "mvlabs/sharengo-cartasi": "dev-master",
        "mockery/mockery": "^0.9.4",
        "mrclay/minify": "2.2.*",
        "natxet/CssMin": "3.0.*",
        "mongodb/mongodb": "^0.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload-dev": {
        "psr-0": {
            "SharengoCore": "module/SharengoCore/src",
            "Cartasi": "module/Cartasi/src"
        }
    }
