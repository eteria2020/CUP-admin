{
    "name": "zendframework/skeleton-application",
    "description": "Skeleton Application for ZF2",
    "license": "BSD-3-Clause",
    "keywords": [
        "framework",
        "zf2"
    ],
    "homepage": "http://framework.zend.com/",
    "repositories": [
        {
            "url": "ssh://git@gitlab.bulweria.com:1022/cup-web/core.git",
            "type": "git"
        },
        {
            "url": "ssh://git@git.mvlabs.it:4222/clients/sharengo-cartasi.git",
            "type": "git"
        },
        {
            "url": "ssh://git@git.mvlabs.it:4222/internals/zf2-multilanguage.git",
            "type": "git"
        }
    ],
    "require": {
        "php": ">=5.4",
        "zendframework/zendframework": "~2.4",
        "bjyoungblood/bjy-authorize": "1.4.0",
        "zf-commons/zfc-user-doctrine-orm": "^1.0",
        "creof/doctrine2-spatial": "^0.0.1",
        "rwoverdijk/assetmanager": "1.4.*",
        "mvlabs/sharengo-coremodule": "dev-master",
        "leafo/lessphp": "0.5.*",
        "mvlabs/sharengo-cartasi": "dev-master",
        "mockery/mockery": "^0.9.4",
        "doctrine/doctrine-mongo-odm-module": "^0.9.1",
        "mrclay/minify": "2.2.*",
        "natxet/CssMin": "3.0.*",
        "mongodb/mongodb": "^0.2.0",
        "mvlabs/zf2-multilanguage": "dev-master",
        "doctrine/migrations": "1.2.2",
        "oro/doctrine-extensions": "1.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "phpmd/phpmd": "@stable"
    },
    "autoload-dev": {
        "psr-0": {
            "SharengoCore": "module/SharengoCore/src",
            "Cartasi": "module/Cartasi/src",
            "BusinessCore": "module/BusinessCore/src",
            "CUPAdminBusiness": "module/CUPAdminBusiness/src"
        }
    }
}
